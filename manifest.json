{
  "manifest_version": 2,
  "name": "Neocities Profile Viewer",
  "version": "1.0",
"description": "Open the profile for any neocities site in a new tab.",
  "homepage_url": "https://github.com/Jett-Secret/NeocitiesProfileViewer",
  "icons": {
    "48": "icons/neocities.ico"
  },
  
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["neocitiesButton.js"]
    }
  ,
  "browser_action": {
  "browser_style": true,
  "default_icon": {
    "16": "icons/neocities.ico",
    "32": "icons/neocities.ico"
  },
  "default_title": "Whereami?",
  "theme_icons": [{
    "light": "icons/neocities.ico",
    "dark": "icons/neocities.ico",
    "size": 16
  }, {
    "light": "icons/neocities.ico",
    "dark": "icons/neocities.ico",
    "size": 32
  }]
}
}